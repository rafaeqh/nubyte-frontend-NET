@model IEnumerable<WebApplicationBlowout.Models.ProductosCompra>

@{
    ViewBag.Title = "ListaProductos";
}

<h2>Productos promoción</h2>

<p>
   
  @Html.ActionLink("Regresar", "ListaPromociones", "Admin")
        
 
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Producto)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Producto)
        </td>
        <td>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Quitar" class="btn btn-default" />
                </div>
            </div>
        </td>
    </tr>
}

</table>
